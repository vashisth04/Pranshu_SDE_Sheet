public class create_an_array {
    public static void main(String[] args) {
        int arr[] = new int [5];
        arr[0]= 10;
        arr[1]= 20;
        arr[2]= 30;
        arr[3]= 40;
        arr[4]= 50;
    }
}


OR 


public class create_an_array {
    public static void main(String[] args) {
    int arr[] = {10,20,30,40,50};
    }
 }

